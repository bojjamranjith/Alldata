package conditional;

public class breakcondition {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       
		
		for(int i=1;i<=4;i++)
		{
			if(i==2)
			{
				System.out.println(i+"->"+"Ranjith");
				continue;
			}
			System.out.println("Ranjith Bojjam");
		}
	}

}
